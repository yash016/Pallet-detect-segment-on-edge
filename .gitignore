# Ignore build, install, and log directories generated by ROS2
/build/
/install/
/log/
/runs/

# Ignore virtual environment folders
myenv/

# Ignore Python compiled files
__pycache__/
*.pyc

# Ignore Jupyter Notebook checkpoints
.ipynb_checkpoints/

# Ignore Docker build cache
*.docker

# Ignore dataset-related files (if generated during runtime or if large)
*.csv
*.json
*.xml

# Ignore any image or video files that are generated or stored temporarily
*.png
*.jpg
*.jpeg
*.mp4

# Ignore YAML config files if they are environment-specific or temporary
*.yaml

# Ignore VS Code workspace-specific settings
.vscode/

